{
  "name": "tinyoutubeadblocker",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A tiny youtube adblocker",
	"permissions": ["webRequest", "webRequestBlocking", "https://www.youtube.com/"],
	"icons": {
		"48": "icons/48.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": "icons/32.png",
		"default_title": "A tiny youtube adblocker"
	}
}
